@{
    Layout = null;
    Response.Headers["X-UA-Compatible"] = "IE=10";
}
<!DOCTYPE html>
<html lang="en" ng-app>
<head>
    <meta charset="utf-8" />
    <title>Home Unknown</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.0-beta.8/angular.min.js"></script>
    <script src="/scripts/controllers/timelineController.js"></script>
</head>
<body>
    <div ng-controller="timelineController">
        <div ng-repeat="timeline in timelines">
            <ul>
                <li>
                    <p>{{timeline.name}}</p>
                </li>
                <li>
                    <p>{{timeline._BeginningYear}}</p>
                </li>
                <li>
                    <img src="{{timeline.pictureurl}}"/>
                </li>
            </ul>
        </div>

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

</body>
</html>
