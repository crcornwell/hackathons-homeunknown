@{
    Layout = null;
    Response.Headers["X-UA-Compatible"] = "IE=10";
}
<!DOCTYPE html>
<html lang="en" ng-app>
<head>
    <meta charset="utf-8" />
    <title>Home Unknown</title>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Content/js/bootstrap.min.js"></script>
    <meta name="viewport" content="width=device-width" />
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.0-beta.8/angular.min.js"></script>
    <script src="/scripts/controllers/timelineController.js"></script>
    
</head>
<body>
    <div class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a href="../" class="navbar-brand">Home Unknown</a>
            </div>
            <div class="navbar-collapse collapse" id="navbar-main">
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="http://builtwithbootstrap.com/" target="_blank">Welcome Steven</a></li>
                    <li><a href="https://wrapbootstrap.com/?ref=bsw" target="_blank"><i class="glyphicon glyphicon-inbox"></i></a></li>
                </ul>

            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div ng-controller="timelineController">
                <ul class="list-inline">
                    <li class="list-inline" ng-repeat="timeline in timelines">
                        <img src="{{timeline.pictureurl}}" />
                        <p>{{timeline.name}}</p>
                        <p>{{timeline._BeginningYear}}</p>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</body>
</html>
